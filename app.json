{
  "expo": {
    "name": "MediApp",
    "slug": "MediApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/svg/MediApp_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/svg/MediApp_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "intentFilters": [],
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "useCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/svg/MediApp_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.carlos_parra.MediApp",
      "networkSecurityConfig": {
        "enabled": true,
        "cleartextTrafficPermitted": true
      }
    },
    "web": {
      "favicon": "./src/assets/svg/MediApp_logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "ff53b409-98f4-487e-b48a-c04bff90218c"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "extraProguardRules": "-keep class com.facebook.react.modules.network.** { *; }"
          }
        }
      ]
    ]
  }
}
